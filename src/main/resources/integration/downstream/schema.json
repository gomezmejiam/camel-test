{
	"$id": "https://example.com/billingReport.schema.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "universities",
	"type": "object",
	"properties": {
		"domains": {
			"type": "array",
			"maxItems": 2,
			"items": {
				"type": "string",
				"pattern": "^[A-Za-z]+(.)(edu|org)$"
			}
		},
		"name": {
			"type": "string",
			"maxLength": 55
		},
		"country": {
			"type": "string",
			"maxLength": 25
		}
	}
}